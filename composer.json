{
    "name": "motniemtin/youtube",
    "description": "Upload a video to YouTube with Laravel 5.",
    "license": "MIT",
    "authors": [
        {
            "name": "Lam Nguyen",
            "email": "motniemtin@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/support": "^5.4",
        "google/apiclient": "^2.2"
    },
    "require-dev": {
        "orchestra/testbench": "~3.2",
        "phpunit/phpunit": "~5.0",
        "phpunit/phpcov": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Motniemtin\\Youtube\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Motniemtin\\Youtube\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Motniemtin\\Youtube\\YoutubeServiceProvider"
            ],
            "aliases": {
                "Youtube": "Motniemtin\\Youtube\\Facades\\Youtube"
            }
        }
    }
}
